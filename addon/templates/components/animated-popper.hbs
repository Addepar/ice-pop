{{unbound _parentFinder}}

{{#if renderInDOM}}
  {{#ember-popper
    popperTarget=_target
    placement=placement
    modifiers=modifiers
    renderInPlace=renderInPlace
    registerAPI="registerAPI"
    class=(concat "ice-animated " (if makeVisible "is-visible " "") popperClass)
  }}
    {{yield}}
  {{/ember-popper}}
{{/if}}
